        WindowStyle="None"
        ResizeMode="NoResize"
        Background="Transparent"
        AllowsTransparency="True"
        WindowStartupLocation="CenterScreen">
    <Border Background="#FF569F39" CornerRadius="30,30,30,30">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="177*"/>
                <ColumnDefinition Width="623*"/>
            </Grid.ColumnDefinitions>
            <Border CornerRadius="30,30,30,30" Grid.Column="0"
                Background="#FF569F39" 
>
                
            </Border>
            <Border CornerRadius="30,30,30,30" Grid.Column="1"
                Background="White" 
>

            </Border>
        </Grid>
    </Border>
</Window>
